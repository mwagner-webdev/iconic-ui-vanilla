{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es2022",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "types": ["node", "mocha", "vite/client"],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
  }
}
